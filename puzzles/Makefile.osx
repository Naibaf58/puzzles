CFLAGS = -O2 -Wall -Werror -g -I./ -Iicons/
LDFLAGS = -framework Cocoa
all: Puzzles fillingsolver galaxiespicture galaxiessolver keensolver \
		latincheck lightupsolver loopysolver magnetssolver mapsolver \
		mineobfusc obfusc patternsolver pearlbench signpostsolver \
		singlessolver slantsolver solosolver tentssolver \
